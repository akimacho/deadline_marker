% layout 'default';
% title 'Welcome';

<h1>Hello, world!</h1>

<table border="1">
% while (my $dl = $Deadline->next) {
<tr>
	<td><%= myEncode_utf8($dl->name) %></td>
	<td><%= myEncode_utf8($dl->event) %></td>
	<td><%= myEncode_utf8($dl->deadline) %></td>
	<td><%= myEncode_utf8($dl->reg_date) %></td>
</tr>
% }
</table>
